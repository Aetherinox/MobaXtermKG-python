    "scripts": {
        "pretest": "npm run lint",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint --fix src/**/*.ts",
        "pretty": "prettier . --check",
        "pretty:fix": "prettier . --write",
        "pretty:ignore": "prettier . --write --ignore-unknown",
        "contrib:add": "all-contributors add",
        "contrib:generate": "all-contributors generate",
        "release": "node release.js",
        "release:generate": "node release.js generate",
        "env:release": "npx --quiet env-cmd --no-override node release.js",
        "env:uuid": "npx --quiet env-cmd --no-override node release.js uuid",
        "env:guid": "npx --quiet env-cmd --no-override node release.js guid",
        "env:version": "node -p require('./package.json').version;"
    },
    "devDependencies": {
        "eslint-plugin-prettier": "^5.2.1",
        "typescript-eslint": "^7.16.1",
        "@typescript-eslint/parser": "^7.16.1",
