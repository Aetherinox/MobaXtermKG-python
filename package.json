        "@rollup/plugin-typescript": "^11.1.6",
        "rollup-plugin-license": "^3.5.2",
        "@types/semver": "^7.5.8",
        "@types/uuid": "^10.0.0",
        "rollup": "^4.18.1",
        "semver": "^7.6.3",
        "uuid": "^10.0.0",
        "all-contributors-cli": "^6.26.1"
    }
}
